<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:id="@android:id/background">
        <shape >
            <corners android:radius="2dp"/>
            <gradient android:angle="270" android:endColor="#ff747674" android:centerY="0.75" android:centerColor="#ff5a5d5a" android:startColor="#ff9d9e9d"/>
        </shape>
        
    </item>
        <item android:id="@android:id/secondaryProgress">
             <clip >
        <shape >
            <corners android:radius="2dp"/>
            <gradient android:angle="0" android:endColor="#ff747674" android:centerY="0.75" android:centerColor="#ff5a5d5a" android:startColor="#ff9d9e9d"/>
        </shape>
        </clip>
    </item>
        <item android:id="@android:id/progress">
        <clip >
        <shape >
            <corners android:radius="5dp"/>
            <gradient android:angle="0" android:endColor="#ff6600" android:centerColor="#cccc00" android:startColor="#99ccff" android:centerX="0.7"/>
        </shape>
        </clip>
    </item>

</layer-list>
